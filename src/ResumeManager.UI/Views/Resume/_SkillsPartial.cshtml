@model ResumeManager.UI.Models.Resume.ResumeCreateViewModel

<form asp-action="SaveCoreSkills" id="SaveCoreSkills">
    <div class="panel-body">
        <div class="form-group">
            <label asp-for="@Model.CoreSkills" class="col-md-2 control-label"></label>
            <div class="col-md-8">
                <input asp-for="@Model.CoreSkills" class="form-control" />
                <span asp-validation-for="@Model.CoreSkills" class="text-danger"></span>                
            </div>
            <div class="col-md-2">
                <button id="addSkill" type="button" class="btn btn-warning form-control">Add Skill</button>
            </div>
        </div>
        <hr class="col-md-12"/>
        <div class="col-md-12 text-center" id="content-placeholder">
        </div>
        <input type="button" value="Save" class="btn btn-warning" id="skills" />
    </div>
</form>

@*Handlebars template*@
<script id="some-template" type="text/x-handlebars-template">
    <table class="table table-hover table-responsive">        
        <tbody>
            {{#each skills}}
            <tr>
                <td>{{name}}</td>
                <td><i class="fa fa-chevron-down" aria-hidden="true"></i></td>                
            </tr>
            {{/each}}
        </tbody>
    </table>
</script>