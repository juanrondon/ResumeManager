@model ResumeManager.UI.Models.Resume.ResumeCreateViewModel

<form id="SkillsForm" class="form-inline">
    <div class="panel-body">
        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <label id="skillLbl" asp-for="@Model.CoreSkills" class="control-label req"></label>
                    <div class="input-group">
                        <input asp-for="@Model.CoreSkills" />
                        <span id="addSkill" class="input-group-addon"><i class="fa fa-plus" aria-hidden="true"></i></span>
                        <span asp-validation-for="@Model.CoreSkills" class="text-danger"></span>
                    </div>

                </div>
            </div>
        </div>
        <hr />
        <div class="col-md-12" id="content-placeholder"></div>
    </div>
</form>


@*Handlebars template*@
<script id="some-template" type="text/x-handlebars-template">
    {{#each skills}}
    <div class="row hov">
        <div class="col-md-6">
            <span class="badge">{{name}}</span>
        </div>
        <div class="col-md-6 icon">
            <span class="iconRemove" data-value="{{name}}"><i class="fa fa-times text-danger" aria-hidden="true"></i></span>
        </div>
    </div>
    {{/each}}


</script>
